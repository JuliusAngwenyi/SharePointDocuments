@{
    ViewBag.Title = "SharePoint Custom Upload";
}

<div class="jumbotron">
    <h2>SharePoint Custom Upload</h2>
</div>
<form action="Upload" enctype="multipart/form-data" method="post" name="id" id="id">
    <label for="file">Upload Image:</label>
    <input type="file" name="file" id="file" /><br><br>
    <input type="submit" value="Upload Image" />
    <br><br>
</form>
@using (Html.BeginRouteForm(null, null, FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <input type="file" name="file" />
    <input type="submit" value="OK" />
}
<div class="row">
    <div class="col-md-12">
        <h2>Alternative to Swagger SharePoint Upload</h2>
    </div>
</div>
